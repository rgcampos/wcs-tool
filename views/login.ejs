    <link rel="stylesheet" href="/styles/01_dependencias/01_bootstrap.min.css">
    <link rel="stylesheet" href="/styles/02_geral/01_config.css">

</head>

<body ng-app="chatBot">


    <div class="container" ng-controller="auth">
        <div class="loginmodal-container">
            <div class="alert alert-success alert-dismissible ng-cloak" role="alert" ng-show="operacaoSucesso">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <strong>Sucesso!</strong> Operação realizada.
            </div>
            <div class="alert alert-danger alert-dismissible ng-cloak" role="alert" ng-show="operacaoFalha">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <strong>Erro!</strong> Falha na operação.
            </div>
            <h1><img src="/images/logo_login.png"></h1><br>
            <form method="POST" ng-submit="processForm()">
                <input type="text" name="chapa" id="chapa" ng-model="formData.chapa" placeholder="Usuário" required autofocus>
                <input type="password" name="senha" id="senha" ng-model="formData.senha" placeholder="Senha" required>
                <input type="submit" class="btn btn-primary btn-block" value="Entrar" id="btnEntrar" name="btnEntrar">
            </form>
            <div id="loading" class="ng-cloak">
                <img src="/images/ajax-loader.gif" alt="Carregando..." title="Carregando...">
            </div>
        </div>
    </div>

    <script src="/js/dependencies/sails.io.js"></script>
    <script src="/js/dependencies/external/01_jquery-3.1.1.min.js"></script>
    <script src="/js/dependencies/external/02_bootstrap.min.js"></script>
    <script src="/js/dependencies/external/03_fontawesome.js"></script>
    <script src="/js/dependencies/external/04_jquery.dataTables.min.js"></script>
    <script src="/js/dependencies/external/13_moment.js"></script>
    <script src="/js/dependencies/angular/main/angular.js"></script>
    <script src="/js/dependencies/angular/main/mainAngular.js"></script>
    <script src="/js/dependencies/angular/main/mainUtils.js"></script>
    <script src="/js/dependencies/angular/controller/authenticate.js"></script>
    <script src="/js/dependencies/angular/controller/home.js"></script>
    <script src="/js/dependencies/angular/controller/menu.js"></script>
</body>

</html>